{
  "name": "Get pages source",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get pages source from a popup",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

"content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["contentscript.css"],
      "js": ["contentscript.js"],
      "run_at": "document_end",
      "all_frames": true
    }],


  "browser_action": {
        "default_icon": "ClickBaitLogo.png",
         "default_popup": "popup.html"
  },


  "permissions": ["tabs", "<all_urls>"]

}

 


